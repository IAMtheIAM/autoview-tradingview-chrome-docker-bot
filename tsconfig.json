{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "dom",
      "es6",
      "es2015"
    ],
    "mapRoot": ".",
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "noEmitHelpers": false,
    "noStrictGenericChecks": true,
    "outDir": "compiled",
    "paths": {},
    "pretty": true,
    "sourceMap": true,
    "sourceRoot": ".",
    "strictNullChecks": false,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node"
    ],
    "outDir": "build"
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    "*.ts"
  ]
}

